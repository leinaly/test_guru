<nav>
  <h1><%= link_to "Test Guru", root_path %></h1>
  <% if logged_in? %>

    <p>Welcome, <b><%= current_user.name %> Guru</b></p>
    <p><%= link_to 'Log out', session_path(current_user.id), method: :delete, data: {confirm: "Are you sure?"} %></p>

  <% else %>
    <p><%= link_to "Sign up", signup_path %></p>
  <% end %>
</nav>
